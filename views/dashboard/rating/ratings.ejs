<section class="section">
  <div class="section-header">
    <h1>Ratings List</h1>
  </div>

  <div class="section-body">
    <div class="row" id="tableID">
      <div class="col-12">
        <div class="card">
          <div class="card-header">
            <h4 class="card-title">Ratings Table</h4>
          </div>
          <div class="table-responsive px-3">
            <table
              id="example"
              class="table table-striped table-bordered"
              style="width: 100%"
            >
              <thead>
                <tr>
                  <th>S.No</th>
                  <th>Doctor Name</th>
                  <th>Patient Name</th>
                  <th>Ratings</th>
                  <th>Comments</th>
                  <!-- <th>Method</th> -->
                </tr>
              </thead>
              <tbody>
                <% ratingsList.forEach((data, i)=>{%>
                <tr>
                  <td><%= i+1%></td>
                  <td><%= data.doctor_details.name %></td>
                  <td><%= data.patient_details.name %></td>
                  <td>
                    <% if(data.rating==1) { %> ★☆☆☆☆ <% } else
                    if(data.rating==2) { %> ★★☆☆☆ <% } else if(data.rating==3) {
                    %> ★★★☆☆ <% } else if(data.rating==4) { %> ★★★★☆ <% } else
                    if(data.rating==0 || data.rating=='') { %> No Rating <% }
                    else if(data.rating==5) { %> ★★★★★ <% } %>
                  </td>
                  <td><%= data.comment%></td>
                </tr>
                <%})%>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

