<section class="app-user-view">
  <div class="app-user-view">
    <div class="user-info-wrapper">
      <div class="row">
        <!-- User Card starts-->
        <div class="col-xl-6 col-lg-8 col-md-7">
          <div class="card user-card">
            <div class="card-body">
              <div class="row">
                <div class="col-12 col-lg-12 mt-2 mt-xl-0 ms-auto text-start">
                  <button type="button" class="border-0 bg-transparent text-secondary  fs-1" onclick="history.back()"><i class="ph-duotone ph-arrow-circle-left"></i></button>
                </div>
                <div class="col-xl-6 col-lg-12 d-flex flex-column justify-content-between border-container-lg">
                  <div class="user-avatar-section">
                    <div class="d-flex justify-content-start">
                      <!-- <img class="img-fluid rounded" src="../../../app-assets/images/avatars/7.png" height="104" width="104" alt="User avatar"> -->
                      <div class="d-flex flex-column ms-1">
                        <div class="user-info mb-1">
                          <h4 class="mb-0">
                            Name </h4>
                          <span class="card-text"><%= update?.name%> </span>
                        </div>
                        <div class="user-info mb-1">
                          <h4 class="mb-0">
                            Email </h4>
                          <span class="card-text"><%= update?.email%> </span>
                        </div>
                        <div class="user-info mb-1">
                          <h4 class="mb-0">
                            Subject </h4>
                          <span class="card-text"><%= update?.subject%> </span>
                        </div>
                        <div class="user-info mb-1">
                          <h4 class="mb-0">
                            Message </h4>
                          <span class="card-text"><%= update?.message%> </span>
                        </div>
                        <div class="user-info mb-1">
                          <h4 class="mb-0">
                            Status </h4>
                          <span class="card-text"><%= update?.status == true ? 'Resolved': 'Pending' %> </span>
                        </div>
                        <!-- <div class="d-flex flex-wrap">
                      <a href="./app-user-edit.html" class="btn btn-primary waves-effect waves-float waves-light">Edit</a>
                      <button class="btn btn-outline-danger ms-1 waves-effect">Delete</button>
                    </div> -->
                      </div>
                    </div>
                  </div>

                </div>
               
              </div>
            </div>
          </div>
        </div>
        <!-- /User Card Ends-->


      </div>
    </div>
  </div>

</section>